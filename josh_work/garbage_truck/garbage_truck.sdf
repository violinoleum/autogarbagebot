          <box>
            <size> 5 0.25 1.3</size>
          </box>
        </geometry>
      </collision>
      <visual name="right_visual">
        <geometry>
          <box>
            <size> 5 0.25 1.3</size>
          </box>
        </geometry>
        <material>
