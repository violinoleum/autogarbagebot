      <child>wheel_back_left</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-1.57</lower>
          <upper>1.57</upper>
        </limit>
      </axis>
    </joint>

    <link name="wheel_front_right">
      <pose> 1.9 -1.1 0.5 -1.5707 0 0</pose>
      <inertial>
        <mass>1000</mass>
      </inertial>
