          <upper>1.57</upper>
        </limit>
      </axis>
    </joint>

    <link name="wheel_back_right">
      <pose> -2 -1.1 0.5 -1.5707 0 0</pose>
      <inertial>
        <mass>1000</mass>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.5</radius>
            <length>0.25</length>
