<?xml version="1.0" ?>
<sdf version="1.10">

  <model name="garbage_truck">
    <static>false</static>

    <!-- Chassis -->
    <link name="chassis">
      <pose>0 0 0.8 0 0 0</pose>
      <inertial>
        <mass>1000</mass>
        <inertia>
          <ixx>500</ixx> <iyy>1200</iyy> <izz>1000</izz>
          <ixy>0</ixy> <ixz>0</ixz> <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="chassis_collision">
        <geometry>
          <box>
            <size>5 2 0.6</size>
          </box>
        </geometry>
      </collision>
      <visual name="chassis_visual">
        <geometry>
          <box>
            <size>5 2 0.5</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Green</name>
          </script>
        </material>
      </visual>

      <!-- IMU Sensor -->
      <sensor name="imu_sensor" type="imu">
        <always_on>1</always_on>
        <update_rate>1</update_rate>
        <visualize>true</visualize>
        <topic>imu</topic>
      </sensor>

      <sensor name="gps_sensor" type="navsat">
        <always_on>true</always_on>
        <update_rate>1</update_rate>
        <topic>gps</topic>
      </sensor>

    </link>

    <!-- Car Structure -->
    <link name="front">
      <pose>2.875 0 1.5 0 0 0</pose>
      <inertial>
        <mass>1</mass>
        <inertia>
          <ixx>500</ixx> <iyy>1200</iyy> <izz>1000</izz>
          <ixy>0</ixy> <ixz>0</ixz> <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="front_collision">
        <geometry>
          <box>
            <size> 0.75 2.5 2</size>
          </box>
        </geometry>
      </collision>
      <visual name="front_visual">
        <geometry>
          <box>
            <size> 0.75 2.5 2</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Blue</name>
          </script>
        </material>
      </visual>
    </link>

    <joint name="joint_front_and_chasis" type="fixed">
      <parent>chassis</parent>
      <child>front</child>
    </joint>

    <link name="bottom">
      <pose> 0 0 1.05 0 0 0</pose>
      <inertial>
        <mass>1</mass>
        <inertia>
          <ixx>500</ixx> <iyy>1200</iyy> <izz>1000</izz>
          <ixy>0</ixy> <ixz>0</ixz> <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="bottom_collision">
        <geometry>
          <box>
            <size> 5 2.5 0.1</size>
          </box>
        </geometry>
      </collision>
      <visual name="bottom_visual">
        <geometry>
          <box>
            <size> 5 2.5 0.1</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Green</name>
          </script>
        </material>
      </visual>
    </link>

    <joint name="joint_bottom_and_chasis" type="fixed">
      <parent>chassis</parent>
      <child>bottom</child>
    </joint>

    <link name="back">
      <pose> -2.375 0 1.75 0 0 0</pose>
      <inertial>
        <mass>1</mass>
        <inertia>
          <ixx>500</ixx> <iyy>1200</iyy> <izz>1000</izz>
          <ixy>0</ixy> <ixz>0</ixz> <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="back_collision">
        <geometry>
          <box>
            <size> 0.25 2.5 1.3</size>
          </box>
        </geometry>
      </collision>
      <visual name="back_visual">
        <geometry>
          <box>
            <size> 0.25 2.5 1.3</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/White</name>
          </script>
        </material>
      </visual>
    </link>

    <joint name="joint_back_and_bottom" type="fixed">
      <parent>bottom</parent>
      <child>back</child>
    </joint>

    <link name="left">
      <pose> 0 1.125 1.75 0 0 0</pose>
      <inertial>
        <mass>1</mass>
        <inertia>
          <ixx>500</ixx> <iyy>1200</iyy> <izz>1000</izz>
          <ixy>0</ixy> <ixz>0</ixz> <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="left_collision">
        <geometry>
          <box>
            <size> 5 0.25 1.3</size>
          </box>
        </geometry>
      </collision>
      <visual name="left_visual">
        <geometry>
          <box>
            <size> 5 0.25 1.3</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/White</name>
          </script>
        </material>
      </visual>
    </link>

    <joint name="joint_left_and_bottom" type="fixed">
      <parent>bottom</parent>
      <child>left</child>
    </joint>

    <link name="right">
      <pose> 0 -1.125 1.75 0 0 0</pose>
      <inertial>
        <mass>1</mass>
        <inertia>
          <ixx>500</ixx> <iyy>1200</iyy> <izz>1000</izz>
          <ixy>0</ixy> <ixz>0</ixz> <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="right_collision">
        <geometry>
          <box>
            <size> 5 0.25 1.3</size>
          </box>
        </geometry>
      </collision>
      <visual name="right_visual">
        <geometry>
          <box>
            <size> 5 0.25 1.3</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/White</name>
          </script>
        </material>
      </visual>
    </link>

    <joint name="joint_right_and_bottom" type="fixed">
      <parent>bottom</parent>
      <child>right</child>
    </joint>

