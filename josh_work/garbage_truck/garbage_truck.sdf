    </link>

    <joint name="joint_bottom_and_chasis" type="fixed">
      <parent>chassis</parent>
      <child>bottom</child>
    </joint>

    <link name="back">
      <pose> -2.375 0 1.75 0 0 0</pose>
      <inertial>
        <mass>1000</mass>
        <inertia>
          <ixx>500</ixx> <iyy>1200</iyy> <izz>1000</izz>
          <ixy>0</ixy> <ixz>0</ixz> <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="back_collision">
        <geometry>
          <box>
            <size> 0.25 2.5 1.3</size>
          </box>
        </geometry>
      </collision>
      <visual name="back_visual">
        <geometry>
          <box>
            <size> 0.25 2.5 1.3</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/White</name>
          </script>
        </material>
      </visual>
    </link>

    <joint name="joint_back_and_bottom" type="fixed">
      <parent>bottom</parent>
      <child>back</child>
    </joint>

    <link name="left">
      <pose> 0 1.125 1.75 0 0 0</pose>
      <inertial>
        <mass>1000</mass>
        <inertia>
          <ixx>500</ixx> <iyy>1200</iyy> <izz>1000</izz>
          <ixy>0</ixy> <ixz>0</ixz> <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="left_collision">
        <geometry>
          <box>
            <size> 5 0.25 1.3</size>
          </box>
        </geometry>
      </collision>
      <visual name="left_visual">
        <geometry>
          <box>
            <size> 5 0.25 1.3</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/White</name>
          </script>
        </material>
      </visual>
    </link>

    <joint name="joint_left_and_bottom" type="fixed">
      <parent>bottom</parent>
      <child>left</child>
    </joint>

    <link name="right">
      <pose> 0 -1.125 1.75 0 0 0</pose>
      <inertial>
        <mass>1000</mass>
        <inertia>
          <ixx>500</ixx> <iyy>1200</iyy> <izz>1000</izz>
          <ixy>0</ixy> <ixz>0</ixz> <iyz>0</iyz>
        </inertia>
