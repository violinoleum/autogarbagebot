<?xml version="1.0" ?>
<sdf version="1.10">
  <model name="garbage_truck">
    <static>false</static>

    <!-- Chassis -->
    <link name="chassis">
      <pose>0 0 0.8 0 0 0</pose>
      <inertial>
        <mass>15000</mass>
        <inertia>
          <ixx>500</ixx> <iyy>1200</iyy> <izz>1000</izz>
          <ixy>0</ixy> <ixz>0</ixz> <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="chassis_collision">
        <geometry>
          <box>
            <size>5 2 0.6</size>
          </box>
        </geometry>
      </collision>
      <visual name="chassis_visual">
        <geometry>
          <box>
            <size>5 2 0.5</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Green</name>
          </script>
        </material>
      </visual>
    </link>

    <!-- Wheels -->
    <link name="wheel_front_left">
      <pose> 1.9 1.1 0.5 -1.5707 0 0</pose>
      <inertial>
        <mass>1000</mass>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.5</radius>
            <length>0.25</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.5</radius>
            <length>0.25</length>
          </cylinder>
        </geometry>
      </visual>
    </link>

    <joint name="joint_wheel_front_left" type="revolute">
      <parent>chassis</parent>
      <child>wheel_front_left</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-1.57</lower>
          <upper>1.57</upper>
        </limit>
      </axis>
    </joint>

    <link name="wheel_center_left">
      <pose> -0.9 1.1 0.5 -1.5707 0 0</pose>
      <inertial>
        <mass>1000</mass>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
